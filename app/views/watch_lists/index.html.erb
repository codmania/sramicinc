<!--start search-container-->
<div class="container-fluid search-container">
  <div class="container">
    <div class="row">

      <div class="col-lg-12 no_pad ">

        <div class="company_users">

          <h1>My WatchList Jobs
            <!--<span class="orange-btn pull-right"> <%= link_to "Add", :class => '' %></span>-->
          </h1>
          <div class="paginations top-pagination">

          <%= will_paginate @WatchList, :inner_window =>2,:outer_window =>0, :renderer=> PaginationHelper::LinkRenderer%>

      </div>

          <div id="no-more-tables">
            <table class="col-sm-12 table-striped table-condensed cf">
              <p id="notice"><%= flash['message']%></p>
              <thead class="cf">
              <tr>
                <th style="display: none"></th>
                <th width="500">Job</th>
                <th width="400">Company</th>
                <th width="" class="table-last-th"></th>
              </tr>
              </thead>
              <tbody>
              <% @watch_lists.each do |watch_list| %>
                  <tr>
                    <td data-title="id" style="display: none">  <%= watch_list.id %></td>
                    <td data-title="Job"><a href="<%= job_path(watch_list.job.id) %>" target="_blank"><%= watch_list.try(:job).try(:title) %></td>
                     <td data-title="Company"><%= watch_list.try(:job).try(:employer).try(:eprofile).try(:company_name) %></td>
                   <td data-title="" class="table-last-td">
                     <ul class="actions_container">
                       <li>
                         <span class="orange-btn pull-right">   <%= link_to "Remove",watch_list_path(watch_list),:method => :delete,
                           :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm",
                               :default => 'Are you sure?')) } %></span>

                       </li>
                     </ul>

                   </td>
                 </tr>
              <% end %>
              </tbody>
            </table>
          </div>
